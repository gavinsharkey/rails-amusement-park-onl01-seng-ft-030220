<h1>Sign In</h1>

<%= form_tag signin_path do %>
  <%= label_tag :user_name, 'Name: ' %>
  <%= select_tag :user_name, options_from_collection_for_select(@users, :id, :name) %><br />

  <%= label_tag :password, 'Password: ' %>
  <%= password_field_tag :password %><br />

  <%= submit_tag 'Sign In' %>
<% end %>